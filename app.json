{
  "expo": {
    "name": "Wadaag",
    "slug": "wadaag",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7e287e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wadaag",
      "buildNumber": "1",
      "config": {
        "googleMapsApiKey": "*****"
      },
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"],
        "NSLocationAlwaysAndWhenInUseUsageDescription": "The Wadaag App uses your location to pick origins, destinations, and find your ride sharing partners",
        "NSLocationAlwaysUsageDescription": "The Wadaag App uses your location to pick origins, destinations, and find your ride sharing partners",
        "NSLocationWhenInUseUsageDescription": "The Wadaag App uses your location to pick origins, destinations, and find your ride sharing partners"
      }
    },
    "android": {
      "package": "com.wadaag",
      "config": {
        "googleMaps": {
          "apiKey": "*****"
        }
      },
      "versionCode": 7,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#7e287e"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "65910a2f-9ee6-453a-a6d5-aa6117932ea8"
      }
    }
  }
}
